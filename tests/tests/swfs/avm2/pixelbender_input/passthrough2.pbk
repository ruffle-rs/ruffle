<languageVersion : 1.0;>

kernel Passthrough2
<   namespace : "Ruffle Tests";
    vendor : "Ruffle";
    version : 1;
    description : "Passthrough shader with 2 channel input";
>
{
    input image2 src;
    output pixel4 dst;

    void evaluatePixel()
    {
        float2 val = sampleNearest(src, outCoord());
        dst = pixel4(val.x, val.y, 0.0, 1.0);
    }
}
